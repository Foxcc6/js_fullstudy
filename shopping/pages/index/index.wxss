.index_swiper image{
    width: 100%;
}
.index_swiper swiper{
    width: 750rpx;
    height: 340rpx;
}
.index_cate{
    display: flex;
  
}
.index_cate navigator{
    flex: 1;
    padding: 20rpx;
}
.index_cate image{
    width: 100%;
}
.floor_title{
    padding: 10rpx 0;
}
 .floor_title image{
    width: 100%;
}
.floor_list{
    overflow: hidden;
}
.list_item{
    float: left;
    width: 33.33%;
    box-sizing: border-box;
}
/* 后四个超链接 */
.list_item:nth-last-child(-n+4) {
    /*计算的出后四张图片的高度*/
    height:27.73vw;
    border-left: 10rpx solid #ffffff;
}

.list_item:nth-child(2), .list_item:nth-child(3)
{
    border-bottom: 10rpx solid #ffffff;
}
.list_item image{
    width: 100%;
    height: 100%;
}
